# AnansiAI Platform Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_ENVIRONMENT=development

# Platform Configuration
VITE_PLATFORM_NAME=AnansiAI
VITE_PLATFORM_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=true

# Authentication
VITE_AUTH_TIMEOUT=3600000
VITE_TOKEN_REFRESH_INTERVAL=300000

# For Production, copy this file to .env and update the values:
# VITE_API_URL=https://your-production-api.com/api
# VITE_ENVIRONMENT=production
# VITE_ENABLE_DEBUG=false
